        <link rel="stylesheet" href="http://libs.cartocdn.com/cartodb.js/v3/themes/css/cartodb.css" />
       
  </head>
  <body>
    <div id="map" style='width:600px;height:400px'></div>
  
    
       <script src="http://libs.cartocdn.com/cartodb.js/v3/cartodb.js"></script>

    <script>

function main () {
      var  map = new L.map('map', {
          zoomControl: true,
          center: [7.50, 3.75],
          zoom: 10
        });
var attr = '<a href="http://mapsnigeriainitiative.wordpress.com">MapsNI</a>,'+'<a href="http://mapbox.com">tiles by mapbox</a>';
   L.tileLayer('http://server.arcgisonline.com/ArcGIS/rest/services/World_Street_map/MapServer/tile/{z}/{y}/{x}.png', {
          attribution: attr
        }).addTo(map);
      
		 var url1 = 'https://mayotunde.cartodb.com/api/v2/viz/57066938-0a33-11e5-a3a9-0e49835281d6/viz.json';
	var url2 = {
	 sql: "SELECT * FROM schoolmapping where type = 'pry'",
	cartocss: "#schoolmapping{marker-fill: #ae7; marker-width: 12; marker-line-color: black; marker-line-width: 1;}"
	}
           cartodb.createLayer(map, url1)
            .addTo(map) 
            .done(function (layer) {
                   }).error(function (){
                                 })
           .on('done', function(layer) {
  layer.getSubLayer(0).set(subLayerOptions);
}).on('error', function() {
});
}
      window.onload = main;
    </script>
  </body>
